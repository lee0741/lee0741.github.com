<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Steven Lee</title>
 <link href="http://lee0741.github.com/atom.xml" rel="self"/>
 <link href="http://lee0741.github.com/blog/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://lee0741.github.com/blog/</id>
 <author>
   <name>Steven Lee</name>
   <email>lee0741@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://lee0741.github.com/blog{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://lee0741.github.com/blog/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>